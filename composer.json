{
    "name": "corneltek/lazyrecord",
    "homepage": "https://github.com/c9s/LazyRecord",
    "keywords": [
        "database",
        "generator",
        "orm",
        "model",
        "collection",
        "sql",
        "fast"
    ],
    "description": "The Fast PHP ORM",
    "require": {
        "php": ">=5.4.0",
        "corneltek/sqlbuilder": "4.0.x-dev",
        "corneltek/cliframework": "^3.0",
        "corneltek/class-template": "^3.0",
        "corneltek/codegen": "3.1.x-dev",
        "corneltek/serializerkit": "^1",
        "corneltek/validationkit": "^1",
        "symfony/yaml": "^2.8",
        "doctrine/inflector": "~1.0",
        "corneltek/configkit": "^1",
        "corneltek/cascading-attribute": "^1.2",
        "pimple/pimple": "^3.0",
        "cypresslab/gitelephant": "^1.0"
    },
    "require-dev": {
        "corneltek/phpunit-testmore": "dev-master"
    },
    "suggest": { "ext-xarray": "*" },
    "bin": [
        "bin/lazy"
    ],
    "support": {
        "issues": "https://github.com/c9s/LazyRecord/issues"
    },
    "license": "MIT",
    "authors": [
        { "name": "Yo-An Lin", "email": "cornelius.howl@gmail.com" }
    ],
    "extra": { "branch-alias": { "dev-master": "4.0.x-dev" } },
    "autoload": {
        "psr-4": {
            "LazyRecord\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AuthorBooks\\": "tests/apps/AuthorBooks/",
            "MetricApp\\": "tests/apps/MetricApp/",
            "PageApp\\": "tests/apps/PageApp/",
            "StoreApp\\": "tests/apps/StoreApp/",
            "TestApp\\": "tests/apps/TestApp/"
        }
    }
}
